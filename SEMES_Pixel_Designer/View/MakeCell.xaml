<Window x:Class="SEMES_Pixel_Designer.View.MakeCell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEMES_Pixel_Designer.View"
        mc:Ignorable="d"
        Title="MakeCell" Height="300" Width="400">
    <Grid>
        <TextBox x:Name="left_info" HorizontalAlignment="Left" Height="20" Margin="132,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

<Page x:Class="SEMES_Pixel_Designer.View.TcpIpLog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SEMES_Pixel_Designer.View" xmlns:viewmodel="clr-namespace:SEMES_Pixel_Designer.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:TcpIpLogViewModel}"
      mc:Ignorable="d" 
      d:DesignHeight="100" d:DesignWidth="800"
      Background="LightGray"
      Title="Console">
    <Grid Margin="3,0,3,3">
        <ListView x:Name="logListView" ItemsSource="{Binding LogMessageList}" VerticalAlignment="Stretch" TextElement.FontSize="11"/>
    </Grid>
</Page>
